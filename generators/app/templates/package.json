{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= name %> serverless api.",
  "private": true,
  "scripts": {
    "test": "./node_modules/.bin/jest --env=node",
    "clean:dev": "./node_modules/.bin/serverless remove --stage dev",
    "deploy:dev": "./node_modules/.bin/serverless deploy --stage dev",
    "logs:dev": "./node_modules/.bin/serverless logs --stage dev --tail -f",

    "clean:prod": "./node_modules/.bin/serverless remove --stage prod",
    "deploy:prod": "./node_modules/.bin/serverless deploy --stage prod",
    "logs:prod": "./node_modules/.bin/serverless logs --stage dev --tail -f"
  },
  "dependencies": {
    "ajv": "^6.1.1",
    "axios": "^0.16.2",
    "js-yaml": "^3.10.0",
    "jsonwebtoken": "^7.4.1"
  },
  "devDependencies": {
    "serverless": "^1.28.0",
    "jwk-to-pem": "^1.2.6",
    "standard": "^10.0.3"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
